{
  "name": "scm2019-layouts",
  "version": "1.0.0",
  "description": "The on-screen graphics used during SpeedSouls Charity Marathon 2019.",
  "homepage": "https://github.com/zoton2/scm2019-layouts#readme",
  "bugs": {
    "url": "https://github.com/zoton2/scm2019-layouts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoton2/scm2019-layouts.git"
  },
  "license": "MIT",
  "author": "zoton2",
  "scripts": {
    "autofix": "run-s autofix:*",
    "autofix:browser": "eslint --fix --ext .ts,.vue src/dashboard && eslint --fix --ext .ts,.vue src/graphics && eslint --fix --ext .ts src/browser_shared",
    "autofix:extension": "eslint --fix --ext .ts src/extension && eslint --fix --ext .d.ts src/types",
    "build": "run-s build:*",
    "build:browser": "cross-env NODE_ENV=production webpack",
    "build:extension": "tsc -b tsconfig.extension.json",
    "clean": "trash node_modules/.cache && trash dashboard && trash graphics && trash extension",
    "schema-types": "nodecg schema-types",
    "start": "node ../..",
    "watch": "run-p watch:*",
    "watch:browser": "webpack -w",
    "watch:extension": "tsc -b tsconfig.extension.json -w"
  },
  "dependencies": {
    "needle": "^3.0.0",
    "obs-websocket-js": "^4.0.3",
    "socket.io-client": "^2.4.0",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@fontsource/roboto": "^4.5.1",
    "@mdi/font": "^6.3.95",
    "@types/clone": "^2.1.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^14.17.29",
    "@types/webpack-env": "^1.16.3",
    "@types/gsap": "^1.20.2",
    "@types/needle": "^2.5.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vue/component-compiler-utils": "^3.2.2",
    "clone": "^2.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.4.0",
    "deepmerge": "^4.2.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-vue": "^7.20.0",
    "fibers": "^5.0.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.4.0",
    "globby": "^12.0.2",
    "gsap": "^2.1.3",
    "html-webpack-plugin": "^5.4.0",
    "mini-css-extract-plugin": "^2.4.3",
    "nodecg-cli": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "~1.32",
    "sass-loader": "^12.2.0",
    "trash-cli": "^4.0.0",
    "ts-loader": "^9.2.6",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.4.4",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-eslint-parser": "^8.0.0",
    "vue-hot-reload-api": "^2.3.4",
    "vue-loader": "^15.9.8",
    "vue-property-decorator": "^9.1.2",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "vuetify": "^2.5.10",
    "vuetify-loader": "^1.7.3",
    "vuex": "^3.6.2",
    "vuex-class": "^0.3.2",
    "vuex-module-decorators": "^1.0.1",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "nodecg": {
    "compatibleRange": "^1.8.1",
    "dashboardPanels": [
      {
        "name": "obs",
        "title": "OBS Panel",
        "file": "obs.html",
        "workspace": "SCM2019",
        "width": 3
      },
      {
        "name": "host",
        "title": "Host",
        "file": "host.html",
        "workspace": "SCM2019",
        "width": 2
      }
    ],
    "graphics": [
      {
        "file": "intermission.html",
        "width": 1280,
        "height": 720
      },
      {
        "file": "4x3-1p.html",
        "width": 1280,
        "height": 720
      },
      {
        "file": "4x3-2p.html",
        "width": 1280,
        "height": 720
      },
      {
        "file": "16x9-1p.html",
        "width": 1280,
        "height": 720
      },
      {
        "file": "16x9-2p.html",
        "width": 1280,
        "height": 720
      },
      {
        "file": "16x9-3p.html",
        "width": 1280,
        "height": 720
      },
      {
        "file": "16x9-4p.html",
        "width": 1280,
        "height": 720
      },
      {
        "file": "16x9-bingo.html",
        "width": 1280,
        "height": 720
      }
    ],
    "assetCategories": [
      {
        "name": "sponsor-logos",
        "title": "Sponsor Logos",
        "allowedTypes": [
          "jpg",
          "jpeg",
          "png",
          "svg"
        ]
      }
    ]
  }
}
